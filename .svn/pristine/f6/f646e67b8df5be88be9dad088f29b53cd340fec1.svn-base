<?xml version="1.0" encoding="UTF-8"?>
<fields>
	<field attr="appCode">
		<text>应用编码</text>
		<check-null>
			<allow-null>1</allow-null>
			<code>40019</code>
		</check-null>
		<check-length>
			<max-length>8</max-length>
			<code>40020</code>
		</check-length>
		<check-exp>
			<exp>^A[0-9]{7}$</exp>
			<code>40020</code>
		</check-exp>
	</field>
	
	<field attr="orderNo">
		<text>业务流水号</text>
		<check-null>
			<allow-null>1</allow-null>
			<code>40001</code>
		</check-null>
		<check-length>
			<max-length>30</max-length>
			<code>40002</code>
		</check-length>
		<check-exp>
			<exp>^[0-9]\d*$</exp>
			<code>40002</code>
		</check-exp>
	</field>
	
	<field attr="userId">
		<text>用户ID</text>
		<check-null>
			<allow-null>1</allow-null>
			<code>40021</code>
		</check-null>
	</field>
	
	<field attr="payCode">
		<text>计费点编码</text>
		<check-null>
			<allow-null>1</allow-null>
			<code>70001</code>
		</check-null>
		<check-length>
			<max-length>16</max-length>
			<code>70002</code>
		</check-length>
		<check-exp>
			<exp>^C[0-9]{7}$</exp>
			<code>70002</code>
		</check-exp>
	</field>
	
	<field attr="appSource">
		<text>应用渠道</text>
